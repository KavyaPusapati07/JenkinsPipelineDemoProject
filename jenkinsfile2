pipeline{
  agent any
  stages{
    stage('build'){
      steps{
        echo 'building'
        bat "mvn clean"
       }
    } 
    stage('test'){
      steps{
        echo 'testing'
        bat "mvn test"
        echo "${params.VERSION}"
      }
    }
  }
  parameters{
    string(name: 'VERSION',defaultValue: '0',description:'version name')
    }
}
    
