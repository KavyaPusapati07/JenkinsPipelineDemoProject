pipeline{
  agent any
  stages{
    stage('build'){
      steps{
        echo 'building'
        bat "mvn clean"
       }
    } 
    stage('test'){
      steps{
        echo "testing "
        bat "mvn test"
        script{
          gv=load("lib.groovy")
          gv.add(12,13)
        }
      }
    }
  }
}
